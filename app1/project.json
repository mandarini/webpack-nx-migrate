{
  "name": "app1",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "app1/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/app1",
        "index": "app1/src/index.html",
        "baseHref": "/",
        "main": "app1/src/main.tsx",
        "tsConfig": "app1/tsconfig.app.json",
        "assets": ["app1/src/favicon.ico", "app1/src/assets"],
        "styles": ["app1/src/styles.css"],
        "scripts": [],
        "isolatedConfig": true,
        "webpackConfig": "app1/webpack.config.js"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "app1/src/environments/environment.ts",
              "with": "app1/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nx/webpack:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "app1:build",
        "hmr": true
      },
      "configurations": {
        "development": {
          "buildTarget": "app1:build:development"
        },
        "production": {
          "buildTarget": "app1:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["app1/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "app1/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
